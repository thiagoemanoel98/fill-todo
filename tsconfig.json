{
  "extends": "expo/tsconfig.base",
  "exclude": ["__tests__/**/*-test.ts"],
  "compilerOptions": {
    "strictNullChecks": true,
    "strict": true,
    "baseUrl": "./",
    "paths": {
      "@components": ["src/components"],
      "@utils": ["src/utils"],
      "@hooks": ["src/hooks"],
      "@routes": ["src/routes"],
      "@screens": ["src/screens"],
      "@services": ["src/services/*"],
      "@styles/*": ["src/styles/*"]
    }
  }
}
